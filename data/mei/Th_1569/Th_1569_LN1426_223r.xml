            <title xml:id="Th_1569_LN1426_223r_title_da962e98" xml:lang="da">Fortørnis icke naar du end seer</title>
            <title type="alternative" xml:id="Th_1569_LN1426_223r_title_8d1a04b5" xml:lang="la">Noli æmulari in malignantibus</title>
            <title type="uniform" xml:id="Th_1569_LN1426_223r_title_674abded" xml:lang="da">Fortørnes ikke, når du end ser</title>
            <contributor xml:id="Th_1569_LN1426_223r_respStmt_5577b899">
               <persName auth.uri="http://www.viaf.org" auth="VIAF" codedval="59249628" role="collector"
