            <title type="alternative" xml:id="Th_1569_LN1426_200v_title_4060ed6f" xml:lang="la">Eructauit</title>
            <title type="uniform" xml:id="Th_1569_LN1426_200v_title_cb3ff65c" xml:lang="da">Hjælp os, O Herre Jesu Krist</title>
