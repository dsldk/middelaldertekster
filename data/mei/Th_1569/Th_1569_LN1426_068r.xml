            <title xml:id="Th_1569_LN1426_068r_title_da962e98" xml:lang="da">O Mennisk begræd din Synd saa stor</title>
            <title type="uniform" xml:id="Th_1569_LN1426_068r_title_107550b0" xml:lang="da">O mennesk, begræd din synd så stor</title>
            <title type="uniform" xml:id="Th_1569_LN1426_068r_title_107220t0" xml:lang="de">O Mensch, bewein dein Sünde gross</title>
            <contributor xml:id="Th_1569_LN1426_068r_respStmt_5577b899">
               <persName auth.uri="http://www.viaf.org" auth="VIAF" codedval="59249628" role="collector" xml:id="Th_1569_LN1426_068r_persName_5eb47039">Hans Thomissøn</persName>
