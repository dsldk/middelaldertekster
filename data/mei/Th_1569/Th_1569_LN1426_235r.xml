            <title xml:id="Th_1569_LN1426_235r_title_da962e98" xml:lang="da">Mit hierte hui gremmer du dig</title>
            <title type="alternative" xml:id="Th_1569_LN1426_235r_title_114e75ea" xml:lang="de">Worum bedröuest du dy myn hert</title>
            <title type="uniform" xml:id="Th_1569_LN1426_235r_title_f9ba9b61" xml:lang="da">Mit hjerte, hvi græmmer du dig</title>
            <title type="uniform" xml:id="Th_1569_LN1426_235r_title_e9f97de9" xml:lang="de">Warum betrübst du dich, mein Herz</title>
            <contributor xml:id="Th_1569_LN1426_235r_respStmt_5577b899">
               <persName auth.uri="http://www.viaf.org" auth="VIAF" codedval="59249628" role="collector"
