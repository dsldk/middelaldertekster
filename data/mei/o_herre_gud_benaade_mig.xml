         <work xml:id="o_herre_gud_benaade_mig_work_7327e799">
            <identifier xml:id="o_herre_gud_benaade_mig_identifier_d1e100" label="Thomissøn 1569">fol. 174v</identifier>
            <identifier label="Jespersen 1573">s. 427</identifier>
